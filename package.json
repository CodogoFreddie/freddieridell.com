{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "prettier": {
    "singleQuote": false,
    "tabWidth": 4,
    "useTabs": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@freddieridell/kebab-case": "0.0.2",
    "@freddieridell/little-bonsai-styles": "^0.5.1",
    "date-fns": "^2.0.0-alpha.27",
    "emotion-theming": "^10.0.14",
    "feather-icons": "^4.22.1",
    "gatsby-plugin-emotion": "^4.1.2",
    "gatsby-plugin-layout": "^1.1.2",
    "gatsby-plugin-sharp": "^2.2.8",
    "gatsby-plugin-sitemap": "^2.2.3",
    "gatsby-remark-copy-linked-files": "^2.1.3",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-prismjs": "^3.3.3",
    "gatsby-remark-smartypants": "^2.1.2",
    "lodash.debounce": "^4.0.8",
    "polished": "^3.4.1",
    "prismjs": "^1.16.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write '**/*.js'",
    "now-build": "npm run build"
  },
  "devDependencies": {
    "gatsby": "^2.13.32",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-remark": "^2.6.8",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
